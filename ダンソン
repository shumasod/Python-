import random

# ダンソンのネタのリスト
danson_list = [
    "ダンソン！",
    "ダンソン！ダンソン！",
    "ダンソン！ダンソン！ダンソン！",
    "ダンソン！ダンソン！ダンソン！ダンソン！",
    "ダンソン！ダンソン！ダンソン！ダンソン！ダンソン！",
    "ダンソン！ダンソン！ダンソン！ダンソン！ダンソン！ダンソン！",
    "ダンソン！ダンソン！ダンソン！ダンソン！ダンソン！ダンソン！ダンソン！",
    "ダンソン！ダンソン！ダンソン！ダンソン！ダンソン！ダンソン！ダンソン！ダンソン！",
]

# メイン処理
while True:
    # ダンソンのネタをランダムに選択
    danson = random.choice(danson_list)

    # ダンソンのネタを出力
    print(danson)
